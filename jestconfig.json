{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "setupFiles": ["./configs/enzyme"],
  "snapshotSerializers": ["enzyme-to-json/serializer"],
  "testPathIgnorePatterns" : [
    "<rootDir>/lib"
  ]
}